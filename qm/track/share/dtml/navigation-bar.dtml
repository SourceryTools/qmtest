<table width="100%" 
       class="navbar"
       border="0"
       cellspacing="0"
       cellpadding="0">
 <tr>
  <td width="45%" height="28">
   &nbsp;&nbsp;
   <b><font size="+1" color="#d0edef">QM</font><font size="+2"
   color="white">Track</font></b>
  </td>

  <td class="navbar" width="12%" align="center">
   <a class="navbar"
      href="<dtml-var expr="web.WebRequest('/track/index', 
                            base=request).AsUrl()">"
   >Main Page</a>
  </td>

  <td class="navbar" width="12%" align="center">
   <a class="navbar"
      href="<dtml-var expr="web.WebRequest('/track/query', 
                            base=request).AsUrl()">"
   >Search</a>
  </td>

  <td class="navbar" width="12%" align="center">
   <a class="navbar"
      href="<dtml-var expr="web.WebRequest('/track/summary', last_query='1', 
            base=request).AsUrl()">"
   >Search Again</a>
  </td>   

  <td class="navbar" width="12%" align="center">
   <a class="navbar"
      href="javascript: void(0);"
      onclick="popup_manual();">Manual</a>
  </td>
 </tr>
</table>
