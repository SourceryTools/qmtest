<dtml-var expr="GenerateXMLHeader()">
 <dtml-var expr="GenerateHtmlHeader('Load Results')">
 <body>
  <form name="load_expected_results" method="post" 
	action="submit-expectations" enctype="multipart/form-data">
   <h3>Load results from file</h3>
   <table border="0" cellpadding="4" cellspacing="4" width="100%">
    <tr class="field">
     <td width="75%">Load results from a file.</td>
     <td><input type="file" 
	        name="expected_results"
                value="<dtml-var expr="qmtest.expectations_file_name">" /></td>
    </tr>
    <tr>
     <td colspan="2" align="right">
      <input type="button" value=" OK " 
             onclick="document.load_expected_results.submit();"/>
      <input type="button" value=" Cancel " onclick="window.close();"/>
     </td>
    </tr>
   </table>
  </form>
 <dtml-var expr="GenerateEndBody()">
</html>

