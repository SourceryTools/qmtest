<dtml-var expr="GenerateXMLHeader()">
 <dtml-var expr="GenerateHtmlHeader('Result Detail')">
 <dtml-var expr="GenerateStartBody()">

 <h3><dtml-var expr="FormatId(result.GetId(), result.GetKind(), 
                              'navigation')"></h3>

 <table border="0" cellpadding="4" cellspacing="4" width="100%">
  <tr class="heading">
   <td width="10%">Outcome</td>
   <td width="90%">Cause</td>
  </tr>
  <tr class ="field">
   <td><span class="outcome"><dtml-var expr="result.GetOutcome()"></span></td>
   <td><dtml-var expr="result.GetCause()"></td>
  </tr>
 </table>

 <table border="0" cellpadding="4" cellspacing="4" width="100%">
  <tr class="heading">
   <td width="20%">Annotation</td>
   <td width="80%">Value</td>
  </tr>
  <dtml-in expr="result.keys()" sort>
   <dtml-let annotation=sequence-item>
    <dtml-if expr="annotation != result.CAUSE">
     <tr class="field">
      <td><dtml-var annotation></td>
      <td><dtml-var expr="result[annotation]"></td>
     </tr>
    </dtml-if>
   </dtml-let>
  </dtml-in>
 </table>

 <dtml-var expr="GenerateEndBody()">
</html>

<dtml-comment><!-- 
  Local Variables:
  mode: xml
  indent-tabs-mode: nil
  fill-column:72
  End:
--></dtml-comment>
