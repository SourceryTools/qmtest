<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE test PUBLIC "-//Software Carpentry//QMTest Test V0.1//EN" "http://www.software-carpentry.com/qm/xml/test.dtd">
<test>
 <class>python.ExecTest</class>
 <argument name='source'>
  <text>
import qm.label
</text>
 </argument>
 <argument name='expression'>
  <text>qm.label.is_valid("abcde") \
           and qm.label.is_valid("ab_xz_efg_0912_bjd__f_") \
           and qm.label.is_valid("_foo_bar", user=0) \
           and not qm.label.is_valid("_foo_bar") \
           and not qm.label.is_valid("") \
           and not qm.label.is_valid("Hello") \
           and not qm.label.is_valid("hello world")</text>
 </argument>
</test>
